{
  "name": "esther-admin",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "ng build",
    "clean": "rimraf dist",
    "precommit": "git add .",
    "doc": "npx @compodoc/compodoc -p compodoc.json -s --port 8081 -n EstherAdmin",
    "commit": "cz",
    "eslint:check": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "eslint:check-html": "eslint \"{src,apps,libs,test}/**/*.html\"",
    "eslint:fix": "npm run eslint:check -- --fix",
    "git:add": "git add .",
    "lint": "npm run eslint:check && npm run prettier:check",
    "ng": "ng",
    "prepare": "husky install",
    "prettier:check": "prettier --check ./**/*.{ts,js,json,*rc}",
    "prettier:write": "prettier --write ./**/*.{ts,js,json,*rc}",
    "sort-package-json": "npx sort-package-json",
    "start": "ng serve",
    "dev": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
